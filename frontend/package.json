{
  "devDependencies": {
    "cssnano": "^4.1.10",
    "sass": "^1.27.0",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "@types/node": "^14.11.10",
    "dotenv": "^8.2.0"
  },
  "scripts": {
    "start": "parcel src/index.html",
    "start:dev": "set NODE_ENV=DEV && npm run start",
    "start:local": "set NODE_ENV=LOCAL && npm run start",
    "build": "parcel build src/pages/*.html -d ../docs --public-url ./",
    "compile": "tsc --noEmit -w",
    "update-types": "cd src/types && (git add . && git commit -m '' && git push) & git pull"
  }
}
